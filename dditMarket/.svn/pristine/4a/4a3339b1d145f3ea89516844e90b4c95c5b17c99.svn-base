@charset "UTF-8";

/* 전체 리스트  */
table{
	margin: auto;
	wudth: 700px;
	border-redius: 5px;
	border-collapse: collapse;
	border-top: none;
}
.header{
	background-color: rgb(218, 231, 255);	/* 헤더 색 */
	text-align: center;						/* 위치 */
}
.table td, .table th{
	border-bottom: 1px lightgray solid;
	height: 30px;	
	font-size: 16px; /* 폰트사이즈 */
}
.num{
	width: 50px;
}
.title{
	width:500px;	
}
.body {
	text-align: center;	
}
.body .title {
	text-align: center;
}

